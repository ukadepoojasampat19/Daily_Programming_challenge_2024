//Floydâ€™s Tortoise and Hare algorithm

#include<bits/stdc++.h>
using namespace std;


int duplicate_number(vector<int>& arr)
{
      int start = arr[0];
      int end = arr[0];

      do
      {
          start = arr[start];
          end = arr[arr[end]];
      }while(start != end);

      start = arr[0];
      while(start != end)
      {
          start = arr[start];
          end = arr[end];
          
      }

      return start;
}
